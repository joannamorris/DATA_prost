@article{parkerTwoMindsAre2019,
  title = {Two Minds Are Not Always Better than One: {{Modeling}} Evidence for a Single Sentence Analyzer},
  shorttitle = {Two Minds Are Not Always Better than One},
  author = {Parker, Dan},
